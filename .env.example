# Salesforce API Configuration (using OPENAI prefix for compatibility)
# Copy this file to .env and fill in your actual values

# API Key for Salesforce Einstein API
OPENAI_API_KEY=your_api_key_here

# Tenant ID for Salesforce Core
OPENAI_TENANT_ID=your_tenant_id_here

# Base URL for Salesforce API
# Use https://test.api.salesforce.com for test environment
# Use https://api.salesforce.com for production
OPENAI_BASE_URL=https://test.api.salesforce.com

# Feature ID for API requests
OPENAI_FEATURE_ID=EinsteinForDevelopers

# Default model to use
# Options: llmgateway__OpenAIGPT5, llmgateway__OpenAIGPT4o, xgen_stream
OPENAI_MODEL=llmgateway__OpenAIGPT5
