{
  "name": "mcpeval",
  "version": "1.0.0",
  "description": "A Node.js TypeScript project with OpenAI SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" \"test/**/*.{ts,tsx,js,jsx,json}\" \"*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" \"test/**/*.{ts,tsx,js,jsx,json}\" \"*.{ts,js,json,md}\"",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/openai": "^2.0.53",
    "@anthropic-ai/sdk": "^0.24.3",
    "ai": "^5.0.78",
    "dotenv": "^17.2.3",
    "openai": "^4.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4",
    "vitest-evals": "^0.5.0"
  }
}
